#!/usr/bin/env bash
set -euo pipefail

patrol build ios --simulator

cd build/ios_integ/Build/Products/Debug-iphonesimulator

# convert to zip archive in this example
zip -r sample.zip Runner.app
zip -r sampleUITests-Runner.zip RunnerUITests-Runner.app

marathon-cloud -api-key "${MARATHON_API_KEY}" -app sample.zip -testapp sampleUITests-Runner.zip -platform iOS
